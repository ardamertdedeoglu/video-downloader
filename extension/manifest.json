{
  "manifest_version": 3,
  "name": "Video Downloader Cookie Sync",
  "version": "1.0.0",
  "description": "YouTube cookie'lerinizi Video Downloader uygulamasına otomatik olarak senkronize edin. +18 videoları indirmek için gerekli.",
  "permissions": [
    "cookies",
    "storage",
    "alarms",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://youtube.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://video-downloader-production-5a88.up.railway.app/*",
      "http://localhost:*/*",
      "http://127.0.0.1:*/*"
    ]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
